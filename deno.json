{
  "tasks": {
    "start": "deno run -A -P src/main.ts",
    "watch": "deno run --watch -A src/main.ts",
    "dev-socket": "deno run --watch -A src/main.ts"
  },
  "vendor": true,
  "imports": {
    "@/": "./src/",
    "@harmony/harmony": "jsr:@harmony/harmony@^2.9.1",
    "@std/assert": "jsr:@std/assert@1"
  },
  "permissions": {
    "default": {
      "env": ["PABLO_*", "PIK_*"],
      "read": ["./node_modules", "./vendor"],
      "net": ["discord.com"]
    }
  }
}
